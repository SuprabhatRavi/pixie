{
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node",
        "config": {
          "distDir": "dist/pixie",
          "buildCommand": "node vercel-build.js",
          "installCommand": "curl -sSf https://sh.rustup.rs | sh -s -- -y && source $HOME/.cargo/env && curl https://rustwasm.github.io/wasm-pack/installer/init.sh -sSf | sh && export PATH=$PATH:$HOME/.cargo/bin && npm install",
          "rustVersion": "stable",
          "maxLambdaSize": "50mb"
        }
      },
      {
        "src": "dist/pixie/browser/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)\\.wasm$",
        "headers": {
          "content-type": "application/wasm",
          "Cache-Control": "public, max-age=31536000, immutable"
        },
        "continue": true
      },
      {
        "src": "/assets/(.*)",
        "headers": {
          "Cache-Control": "public, max-age=31536000, immutable"
        },
        "continue": true
      },
      { "handle": "filesystem" },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  