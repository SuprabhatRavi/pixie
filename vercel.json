{
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist/pixie",
          "buildCommand": "node vercel-build.js",
          "installCommand": "curl -sSf https://sh.rustup.rs | sh -s -- -y && curl https://rustwasm.github.io/wasm-pack/installer/init.sh -sSf | sh && npm install",
          "rustVersion": "stable"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)\\.wasm$",
        "headers": {
          "content-type": "application/wasm",
          "Cache-Control": "public, max-age=31536000, immutable"
        },
        "continue": true
      },
      {
        "src": "/assets/(.*)",
        "headers": {
          "Cache-Control": "public, max-age=31536000, immutable"
        },
        "continue": true
      },
      {
        "handle": "filesystem"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }