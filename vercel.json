{
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist/pixie",
          "buildCommand": "npm run build:wasm && npm run build",
          "installCommand": "npm install && npm install -g wasm-pack",
          "rustVersion": "stable"
        }
      }
    ],
    "routes": [
        {
          "src": "/(.*)",
          "dest": "/"
        }
    ]
  }