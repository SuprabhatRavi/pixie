{
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist/pixie",
          "buildCommand": "node vercel-build.js",
          "installCommand": "curl https://rustwasm.github.io/wasm-pack/installer/init.sh -sSf | sh && npm install",
          "rustVersion": "stable"
        }
      }
    ],
    "routes": [
        {
          "src": "/(.*)\\.wasm$",
          "headers": {
            "content-type": "application/wasm"
          },
          "continue": true
        },
        {
          "handle": "filesystem"
        },
        {
          "src": "/(.*)",
          "dest": "/"
        }
    ]
}