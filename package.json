{
  "name": "pixie",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:wasm": "wasm-pack build --target web ./wasm && node scripts/copy-wasm.js",
    "build:wasm:prod": "wasm-pack build --target web ./wasm && node scripts/copy-wasm-prod.js",
    "watch:wasm": "nodemon --watch ./wasm/src -e rs --exec \"npm run build:wasm\"",
    "start": "concurrently \"npm run watch:wasm\" \"ng serve\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/router": "^19.0.0",
    "assert": "^2.1.0",
    "browser-assert": "^1.2.1",
    "fabric-pure-browser": "^5.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^19.0.0",
    "@angular-devkit/build-angular": "^19.0.6",
    "@angular/cli": "^19.0.6",
    "@angular/compiler-cli": "^19.0.0",
    "@types/fabric": "^5.3.0",
    "@types/jasmine": "~5.1.0",
    "concurrently": "^8.2.0",
    "jasmine-core": "~5.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "nodemon": "^3.0.1",
    "typescript": "~5.6.2"
  },
  "browser": {
    "assert": "browser-assert"
  }
}
